<!-- Content Header (Page header) -->
<div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark"></h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
                {% for bread in breadcrumb %}
                  {% if bread.active %}
                    <li class="breadcrumb-item active" aria-current="page"><span>{{ bread.label }}</span></li>
                  {% else %}
                    <li class="breadcrumb-item"><a href="{{ bread.url }}">{{ bread.label }}</a></li>
                  {% endif %}
                {% endfor %}
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>